name: mlflow-ci-project

entry_points:
  main:
    parameters:
      n_estimators:
        type: int
        default: 100
      max_depth:
        type: int
        default: 10
      dataset:
        type: path
        default: day_wise_processed.csv
    command: >
      python modelling.py
      {n_estimators}
      {max_depth}
      {dataset}
